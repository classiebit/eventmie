{
    "name": "classiebit/eventmie",
    "version": "3.0.0",
    "description": "Run your own Events business with Eventmie Pro. Use it as event ticket selling website or event management platform on your own domain.",
    "keywords": [
        "AI event creation",
        "event management system",
        "mobile scanner",
        "multi-currency",
        "online booking",
        "POS",
        "on-site sales",
        "recurring events",
        "reserved seating",
        "self-hosted",
        "stripe connect marketplace",
        "ticketing system",
        "white-label event software",
        "eventmie pro",
        "online events",
        "zoom",
        "google meet",
        "jio meet",
        "events",
        "event ticket selling website",
        "event management platform",
        "event-management",
        "event-planning",
        "laravel",
        "laravel-package",
        "voyager",
        "vuejs"
    ],
    "license": "MIT",
    "homepage": "https://classiebit.com/eventmie",
    "support": {
        "issues": "https://github.com/classiebit/eventmie/issues",
        "source": "https://github.com/classiebit/eventmie"
    },
    "authors": [
        {
            "name": "Classiebit",
            "email": "info@classiebit.com"
        }
    ],
    "require": {
        "illuminate/support": "~8.0|~9.0|~10.0|~11.0",
        "tcg/voyager": "^1.8",
        "tightenco/ziggy": "^2.5",
        "league/flysystem-aws-s3-v3": "^3.29",
        "laravel/ui": "^4.6",
        "devrabiul/laravel-cookie-consent": "^1.0"
    },
    "require-dev": {
        "laravel/framework": "~8.0|~9.0|~10.0|~11.0"
    },
    "autoload": {
        "psr-4": {
            "Classiebit\\Eventmie\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Classiebit\\Eventmie\\EventmieServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
